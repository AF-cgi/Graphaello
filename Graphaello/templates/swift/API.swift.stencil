// MARK: - {{ api.name }}

struct {{ api.name }} {
    let client: ApolloClient

    {{ api.query.fields|code:"static" }}
    {{ api.types|code }}
}

{% for type in api.types %}
extension GraphQLFragmentPath where UnderlyingType == {{ api.name }}.{{ type.name }} {
    {{ type.fields|code }}
}

extension GraphQLFragmentPath where UnderlyingType == {{ api.name }}.{{ type.name }}? {
    {{ type.fields|code:"optional" }}
}
{% endfor %}
